{
  "name": "dpripa/my-wp-plugin",
  "version": "0.9.0",
  "type": "wordpress-plugin",
  "description": "Just my WordPress plugin boilerplate",
  "homepage": "https://github.com/dpripa/my-wp-plugin",
  "license": "GPL <https://github.com/dpripa/my-wp-plugin/blob/main/LICENSE>",
  "authors": [
    {
      "name": "Some Developers"
    },
    {
      "name": "Dmitry Pripa",
      "email": "i@dpripa.com",
      "homepage": "https://github.com/dpripa"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "My_Plugin\\": "includes/"
    }
  },
  "require": {
    "php": "^7.2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3.1",
    "wp-coding-standards/wpcs": "^2.2.1"
  },
  "scripts": {
    "optimize": "composer dump-autoload --optimize",
    "wpcs": "phpcs"
  }
}
